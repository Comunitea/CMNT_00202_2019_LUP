<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="view_template_form_putaway" model="ir.ui.view">
        <field name="name">inherit.putaway.product.template.product.form</field>
        <field name="model">product.template</field>
        <field name="inherit_id" ref="stock_putaway_product_form.view_template_form_putaway"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='product_tmpl_putaway_ids']/tree" position="inside">
                <field name="max_qty"/>
            </xpath>
        </field>
    </record>

</odoo>
