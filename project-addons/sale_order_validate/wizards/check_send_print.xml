<odoo>
    <record id="check_send_print_wizard" model="ir.ui.view">
        <field name="name">Check Send print</field>
        <field name="model">check.send.print.wiz</field>
        <field name="arch" type="xml">
            <form string="Revisar presupuesto">
                
                <field name="exception_msg" colspan="2" nolabel="1"/>
                <footer>
                    <button string="Continue"
                            class="oe_highlight"
                            name="button_continue"
                            type="object"
                            />
                    <button string="Cancel"
                            class="oe_link"
                            special="cancel"
                            />
                </footer>
            </form>
        </field>
    </record>
</odoo>
